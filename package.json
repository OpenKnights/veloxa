{
  "name": "veloxa",
  "version": "0.3.0",
  "description": "Veloxa is a fast, native request library based on the Fetch API.",
  "type": "module",
  "keywords": [
    "veloxa",
    "fetch",
    "request"
  ],
  "license": "MIT",
  "homepage": "https://github.com/OpenKnights/veloxa#readme",
  "bugs": {
    "url": "https://github.com/OpenKnights/veloxa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenKnights/veloxa.git"
  },
  "author": "king-3 <w2196592083@gmail.com>",
  "files": [
    "dist/"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown --clear --dts",
    "build:mock": "pnpm --filter mock-server run build",
    "test": "vitest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "destr": "^2.0.5",
    "devix": "^1.0.4",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@king-3/eslint-config": "^1.0.5",
    "@types/node": "^24.5.2",
    "bumpp": "^10.2.3",
    "create-mock-server": "^0.1.0",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
