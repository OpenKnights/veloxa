{
  "name": "veloxa",
  "version": "1.0.3",
  "description": "Veloxa is a fast, native request library based on the Fetch API.",
  "type": "module",
  "keywords": [
    "veloxa",
    "fetch",
    "http",
    "request",
    "client",
    "api",
    "typescript",
    "lightweight"
  ],
  "license": "MIT",
  "homepage": "https://github.com/OpenKnights/veloxa#readme",
  "bugs": {
    "url": "https://github.com/OpenKnights/veloxa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenKnights/veloxa.git"
  },
  "author": "king-3 <w2196592083@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "devix": "^1.0.4",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@king-3/eslint-config": "^1.0.8",
    "@king-3/prettier-config": "^1.0.2",
    "@types/node": "^24.5.2",
    "bumpp": "^10.2.3",
    "create-mock-server": "^0.1.4",
    "eslint": "^9.34.0",
    "h3": "^1.15.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
